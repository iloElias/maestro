{
    "name": "ilias/choir",
    "description": "A PHP HTTP requests handler template",
    "type": "project",
    "license": "MIT",
    "keywords": ["PHP", "HTTP", "requests", "handler", "template"],
    "homepage": "https://github.com/ilias/choir",
    "autoload": {
      "psr-4": {
        "Ilias\\Choir\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "scripts": {
      "test": "phpunit"
    },
    "authors": [
      {
        "name": "Murilo Elias",
        "email": "murilo7456@gmail.com",
        "homepage": "https://ilias.dev",
        "role": "Developer"
      }
    ],
    "require": {
      "php": "^7.4 || ^8.0",
      "ilias/rhetoric": "^1.0",
      "ilias/opherator": "^1.0",
      "ilias/maestro": "^1.0",
      "ilias/dotenv": "^1.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.0"
    },
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
  }
  