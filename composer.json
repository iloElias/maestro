{
    "name": "ilias/maestro",
    "type": "library",
    "version": "2.0.0",
    "description": "A PHP object-oriented postgres database manager",
    "keywords": [
        "postgres",
        "orm",
        "pgsql"
    ],
    "homepage": "https://github.com/iloElias/maestro",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ilias\\Maestro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maestro\\Tests\\": "tests/",
            "Maestro\\Example\\": "example/"
        }
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit tests --colors",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix"
    },
    "authors": [
        {
            "name": "Murilo Elias",
            "email": "murilo7456@gmail.com"
        },
        {
            "name": "Divino Henrique",
            "email": "dhenriquearantes@gmail.com"
        }
    ],
    "require": {
        "ilias/dotenv": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "friendsofphp/php-cs-fixer": "*"
    },
    "minimum-stability": "dev"
}